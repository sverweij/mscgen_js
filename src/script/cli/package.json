{
  "name": "mscgenjs-cli",
  "version": "1.2.4",
  "description": "Command line interface to mscgen_js",
  "main": "src/mscgen.js",
  "preferGlobal": true,
  "bin": {
    "mscgen_js": "bin/mscgen_js",
    "mscgenjs": "bin/mscgen_js",
    "mscgen.js": "bin/mscgen_js"
  },
  "dependencies": {
    "amdefine": "1.0.0",
    "phantomjs": "1.9.19",
    "requirejs": "2.1.22"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "chai-xml": "0.3.1",
    "codeclimate-test-reporter": "0.1.1",
    "codecov.io": "0.1.6",
    "commander": "2.9.0",
    "istanbul": "0.4.1",
    "js-makedepend": "0.1.10",
    "jscs": "2.7.0",
    "jshint": "2.8.0",
    "mocha": "2.3.4",
    "npm-check-updates": "2.5.4",
    "nsp": "2.1.0",
    "plato": "1.5.0"
  },
  "scripts": {
    "test": "mocha -R dot --timeout 4000 --recursive test",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- --timeout 8000 --recursive test",
    "nsp": "nsp check",
    "plato": "plato -r -d platoreports -x \"test\" .",
    "lint": "jshint . --verbose --show-non-errors",
    "codeclimate-cover-submit": "node node_modules/codeclimate-test-reporter/bin/codeclimate.js < coverage/lcov.info",
    "codecov-submit": "node node_modules/codecov.io/bin/codecov.io.js < coverage/lcov.info",
    "npm-check-updates": "ncu --upgrade",
    "jscs": "jscs src/script",
    "start": "node src/mscgen.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sverweij/mscgenjs-cli"
  },
  "author": "Sander Verweij",
  "license": "GPL-3.0",
  "keywords": [
    "mscgen",
    "sequence chart",
    "sequence diagram",
    "xu",
    "msgenny"
  ],
  "engines": {
    "node": ">=4.0"
  },
  "bugs": {
    "url": "https://github.com/sverweij/mscgenjs-cli/issues"
  },
  "homepage": "https://mscgenjs.github.io/"
}
